<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.mafe0002" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mafe0002sGiftrApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/giftrIcon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/giftrIcon-48-mhdpi.png" />
        <icon density="hdpi" src="res/icon/android/giftrIcon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/giftrIcon-96-xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="res/icon/ios/giftrIcon-57.png" />
        <icon src="res/icon/ios/giftrIcon-57-2x.png" />
        <icon src="res/icon/ios/giftrIcon-72.png" />
        <icon src="res/icon/ios/giftrIcon-72-2x.png" />
    </platform>
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^7.0.0" />
</widget>
